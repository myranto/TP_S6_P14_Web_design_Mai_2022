<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite
        PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>
    <rule>
        <from>^/produit.html$</from>
        <to>/detail?id=1</to>
    </rule>
    <rule>
        <from>^/?.*/?.*-([_a-z0-9]*)-([_a-z0-9]*).html$</from>
        <to>/</to>
    </rule>
</urlrewrite>
